{
    "name": "paqtcom/external-api-call-client",
    "description": "Setup code for making a client to an external API client",
    "type": "library",
    "autoload": {
        "psr-4": {
            "PaqtCom\\ExternalApiCallClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PaqtCom\\Tests\\ExternalApiCallClient\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pieter Jordaan",
            "email": "pieter.jordaan@paqt.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8.2",
        "phpunit/phpunit": "^9.5.23",
        "phpspec/prophecy-phpunit": "^v2.0.1",
        "jangregor/phpstan-prophecy": "^1.0",
        "symfony/mime": "6.1.*"
    }
}
